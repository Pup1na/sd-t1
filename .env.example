# Variables de entorno para el proyecto
# Copia este archivo a .env y configura tus valores

# API Key de Gemini (obtener de https://aistudio.google.com/)
GEMINI_API_KEY=your_gemini_api_key_here

# Configuración de base de datos
DB_HOST=postgres
DB_PORT=5432
DB_NAME=yahoo_answers
DB_USER=admin
DB_PASSWORD=password

# Configuración de Redis
REDIS_HOST=redis
REDIS_PORT=6379

# URLs de servicios (para desarrollo)
STORAGE_URL=http://localhost:8001
CACHE_URL=http://localhost:8002
SCORE_URL=http://localhost:8003
LLM_URL=http://localhost:8004
